<!DOCTYPE html>
<html>
<head>
  <title>Rantly</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="header">
  <div class="logo">
    <%= link_to "Rant.ly", root_path %>
  </div>
  <div class="join">
    <%= link_to "Join", register_path, method: 'get' %>
  </div>
  <div class="link">
    <%= link_to "login", signin_path %>
   </div>

 </div>
<main>
   <div class="wrapper">

<%= yield %>

     </div>
  </main>

</body>
</html>
